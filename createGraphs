for(i in 1:length(loc)) {
  conjPC_data <- read.table(paste("/home/ichproject/szimmerman/PCAtables4/",toString(loc[i]),sep=""),header=T,sep=",")
  if(grepl("starvation",toString(loc[i]))) {
    g_range <- range(0,conjPC_data$rep1,conjPC_data$rep2, conjPC_data$rep3)
    } else {
    g_range <- range(0,conjPC_data$rep1,conjPC_data$rep2)
    }
  png(filename=paste("/home/ichproject/szimmerman/PCAgraphs4/",toString(loc[i]),".png",sep=""),height=295,width=300,bg="white")
  plot(conjPC_data$rep1,type="o",col="blue",ylim=g_range,axes=TRUE,ann=TRUE)
  box()
  lines(conjPC_data$rep2,type="o",col="red")
  if(grepl("starvation",toString(loc[i]))) {
    lines(conjPC_data$rep3,type="o",col="green")
  }
  title(main=toString(loc[i]))
  dev.off()
}
q()
