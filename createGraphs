  conjPC_data <- read.table(paste("/home/ichproject/szimmerman/PCAtablesCombo/",toString(loc[i]),sep=""),header=T,sep=",")
  g_range <- range(0,conjPC_data$rep1)
  png(filename=paste("/home/ichproject/szimmerman/PCAgraphsCombo/",toString(loc[i]),".png",sep=""),height=295,width=300,bg="white")
  plot(conjPC_data$rep1,type="o",col="blue",ylim=g_range,axes=TRUE,ann=TRUE)
  box()
  title(main=toString(loc[i]))
  dev.off()
  }
